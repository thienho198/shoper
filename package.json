{
  "name": "react-news-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "let-dev": "run-script-os",
    "let-build": "run-script-os",
    "let-dev:win32": "npm run dev",
    "let-dev:darwin:linux": "npm run dev",
    "let-build:win32": "npm run build:client && timeout 1 && npm run build:server",
    "let-build:darwin:linux": "npm run build:client && sleep 1 && npm run build:server",
    "build:client": "webpack --progress --config webpack.client.prod.js --mode production",
    "build:server": "webpack --config webpack.server.js",
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "start": "node build/bundle.js",
    "heroku-postbuild": "npm install && npm run let-build",
    "precommit": "pretty-quick --staged"
  },
  "author": "ilker ALTIN",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/polyfill": "^7.12.1",
    "axios": "0.21.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^6.1.1",
    "concurrently": "5.3.0",
    "copy-to-clipboard": "^3.3.1",
    "currency.js": "^2.0.3",
    "express": "4.17.1",
    "joi": "^17.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nodemon": "2.0.6",
    "npm-run-all": "4.1.5",
    "prop-types": "^15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-lazy-load-image-component": "^1.5.1",
    "react-loadable": "^5.5.0",
    "react-loadable-ssr-addon": "^1.0.0",
    "react-redux": "7.2.2",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.2.0",
    "react-toastify": "^6.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "serialize-javascript": "5.0.1",
